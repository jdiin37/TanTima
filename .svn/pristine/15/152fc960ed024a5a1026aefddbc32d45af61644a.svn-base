/**panel 樣式***/
.panel-info {
  border-color: #4B1E8A;
  border-width: 1px;
  border-radius: 0px;
}
.panel-info > .panel-heading {
  color: #49116D;/***標頭顏色 原 #000 改為  #49116D 修改 1070319 **/
  border-color: #CCBEE5;
