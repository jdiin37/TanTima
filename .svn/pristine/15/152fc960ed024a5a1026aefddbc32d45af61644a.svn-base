/***button 樣式***/
/*button 按鈕*/
.btn-primary {
    background-color: #611BBD;
    background: linear-gradient(to bottom, rgba(150,80,231,1) 0%, rgba(150,80,231,1) 50%, rgba(111,41,172,1) 100%);
    border: 0px solid transparent;
    font-size: 18px;
    color: #fff;
    margin-bottom: 2px;
    margin-top: 2px;
 
}
.btn-primary:focus,
.btn-primary.focus {
  color: #fff;
  /**background: linear-gradient(to bottom, #FEFEFE 50%, #DBDCDB 100%);**/
  border: 0px solid transparent;
  background:linear-gradient(to bottom, rgba(111,41,172,1) 0%, rgba(111,41,172,1) 50%, rgba(150,80,231,1) 100%);
  /**border-color: #727171; **/
}
.btn-primary:hover {
  color: #fff;
  background-image: linear-gradient(to bottom, #6F28AC 50%, #954FE6 100%);
  border: 0px solid transparent;
  /**background:linear-gradient(to bottom, rgba(201,202,201,1) 100%) rgba(255,255,255,1) 0%, rgba(255,255,255,1) 50%, ;**/
  /**border-color: #727171; **/
}
.btn-primary:active,
.btn-primary.active,
.open > .dropdown-toggle.btn-primary {
  color: #fff;
  /**background: linear-gradient(to bottom, #FEFEFE 50%, #DBDCDB 100%);**/
  background:linear-gradient(to bottom, rgba(111,41,172,1) 0%, rgba(111,41,172,1) 50%, rgba(150,80,231,1) 100%);
  border: 0px solid transparent;
  /**border-color: #727171; **/
}
.btn-primary:active:hover,
.btn-primary.active:hover,
.open > .dropdown-toggle.btn-primary:hover,
.btn-primary:active:focus,
.btn-primary.active:focus,
.open > .dropdown-toggle.btn-primary:focus,
.btn-primary:active.focus,
.btn-primary.active.focus,
.open > .dropdown-toggle.btn-primary.focus {
  color: #fff;
  background: linear-gradient(to bottom, #742AB5 50%, #954CE4 100%);
  border: 0px solid transparent;	
  /**border-color: #727171;**/ 
}
.btn-primary:active,
.btn-primary.active,
.open > .dropdown-toggle.btn-primary {
  background: linear-gradient(#5F2493, #9753EA, #5F2493);
}
.btn-primary.disabled:hover,
.btn-primary[disabled]:hover,
fieldset[disabled] .btn-primary:hover,
.btn-primary.disabled:focus,
.btn-primary[disabled]:focus,
fieldset[disabled] .btn-primary:focus,
.btn-primary.disabled.focus,
.btn-primary[disabled].focus,
fieldset[disabled] .btn-primary.focus {
  background-color: #611BBD; 
  border-color: #130269; 
}


