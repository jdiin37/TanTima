  padding: 8px 10px 4px 15px; /**測試 縮小間距 1070326 add***/
  border-radius: 0px;
  
}
.panel-info > .panel-heading + .panel-collapse > .panel-body {
  /***border-top-color: #E0CEFF;**/
  border:1px solid #CDBDEC;
