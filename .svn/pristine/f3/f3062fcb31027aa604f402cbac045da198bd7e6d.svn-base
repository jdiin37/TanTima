/** 1.藥品/2.處置/3.材料**/
function formatUDPriceType(cellValue, options, rowObject) {
	
	switch(cellValue){
	case 1:
		return "藥品";
		break;
	case 2:
		return "處置";
		break;
	case 3:
		return "材料";
		break;
		default:
		
	}

        		  
};

