/***jqGrid CSS**/
.ui-jqgrid{
	border:1px solid #E1D2ED;
	font-size:18px;/**可能會導致 Grid的格子變大**/
	font-weight:bold;
	color:#000;
}

.table>thead>tr.ui-jqgrid-labels >th {
	background-image: linear-gradient(to bottom, #F0E3FD 0, #E1C5FF 100%);
}

.ui-jqgrid .ui-jqgrid-htable th {
    height: 35px;
    padding: 2px;
    text-align:center;
     min-height:25px;
}
.ui-jqgrid .ui-jqgrid-htable th div {
    height: 35px;
    text-align:center;
    padding-top:1px;
    min-height:25px;
} 

.ui-jqgrid-btable>tbody>tr:nth-child(odd)>td {
	/**background-color: #e4e4e4;**/
	background-color:#F2F2F2;
}

.ui-jqgrid-btable>tbody>tr:nth-child(even)>td {
	/**background-color: #e4e4e4;**/
	background-color:#FFF;
}


.table-bordered>tbody>tr>td, .table-bordered>tbody>tr>th, .table-bordered>tfoot>tr>td, .table-bordered>tfoot>tr>th, .table-bordered>thead>tr>td, .table-bordered>thead>tr>th{
	border:1px solid #DCC8EE;
}

