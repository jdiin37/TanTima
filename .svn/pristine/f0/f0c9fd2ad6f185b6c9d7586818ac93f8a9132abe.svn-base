/**CSS Scrollbar 設定 for Chrome***/
::-webkit-scrollbar {
    width: 16px;
}

::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3); 
    border-radius: 8px;
    background:#BCA3E3;
}

::-webkit-scrollbar-thumb {
    border-radius: 8px;
    background:#E6DBFB; 
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.5);
    /**-webkit-box-shadow: inset 0 0 6px #BCA3E3;  **/
}


::-moz-scrollbar {
    width: 16px;
}

::-moz-scrollbar-track {
    -moz-box-shadow: inset 0 0 6px rgba(0,0,0,0.3); 
    border-radius: 8px;
    background:#BCA3E3;
}

::-moz-scrollbar-thumb {
    border-radius: 8px;
    background:#E6DBFB; 
    -moz-box-shadow: inset 0 0 6px rgba(0,0,0,0.5);
    
}


