	<script>
	
	var showTab = 8;
	var defaultLi = $('#ul-TPRTabSwitch > li').eq(showTab).addClass('active');
	$(defaultLi.find('a').attr('href')).siblings().hide();
	
		$(document).on('click','#Modal_Custom .btn-OK',function(event) {	
			$('#editStool').val($('#stoolRecordText').html());
		});
	
		$('#Modal_Custom').on('show.bs.modal', function (event) { //彈跳視窗 跳出時
			$("#stoolTimeSelect").val("0").trigger('change');	
			$("#stoolTypeSelect").val("").trigger('change');	
			var val = $('#editStool').val();
			if(val.length>0&&val.indexOf(';',0)==-1){
			   $('#stoolRecordText').html(val+";");	
			}else{
			   $('#stoolRecordText').html(val);	
			}
		  	
			
